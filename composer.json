{
    "name": "syntax/blade-icons",
    "version": "1.0.0",
    "description": "An easy way inline SVG images in your Blade templates.",
    "type": "library",
    "license": "MIT",
    "require": {
        "laravel/framework": "^10.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "authors": [
        {
            "name": "swara",
            "email": "swara09@gmail.com"
        }
    ],
    "conflict": {
        "laravel/framework": "<10.0",
        "orchestra/testbench": "<8.0"
    },
    "autoload": {
        "psr-4": {
            "Syntax\\Icons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syntax\\Icons\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Syntax\\Icons\\IconServiceProvider"
            ]
        }
    }
}
